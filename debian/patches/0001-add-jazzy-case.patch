   add_compile_definitions(IRON)
+elseif($ENV{ROS_DISTRO} MATCHES "jazzy")
+  add_compile_definitions(JAZZY)
 elseif($ENV{ROS_DISTRO} MATCHES "rolling")
   add_compile_definitions(ROLLING)
 endif()
